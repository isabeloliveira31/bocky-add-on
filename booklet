javascript:(function(){ const iframe= document.createElement('iframe'); iframe.id = 'bocky-iframe' iframe.src = 'https://isabeloliveira31.github.io/bocky-add-on/bocky-add-on.html'; iframe.allowTransparency = true; iframe.scrolling = 'no'; Object.assign(iframe.style, { position: 'fixed', border: 'none', zIndex: '2147483647', height: '510px', width: '350px', bottom: 'min(50px, 5%)', right: 'min(40px, 4%)', background: 'none', backgroundColor: 'transparent'});  document.body.appendChild(iframe); window.addEventListener('message', (event) => { const iframe = document.getElementById('bocky-iframe'); if (!iframe) return; if (event.data.type === 'expand-bocky') { iframe.style.width = event.data.width; iframe.style.height = event.data.height; } else if (event.data.size === 'collapsed') { iframe.style.width = event.data.radius; iframe.style.height = event.data.radius; }}); })();